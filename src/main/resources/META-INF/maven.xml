<idea-plugin>

    <extensions defaultExtensionNs="spring.assistant.initializr">
        <moduleBuilderPostProcessor
                implementation="in.oneton.idea.spring.assistant.plugin.initializr.MavenModuleBuilderPostProcessor"/>
    </extensions>


    <project-components>
        <component>
            <interface-class>
                in.oneton.idea.spring.assistant.plugin.component.MavenReIndexingDependencyChangeSubscriber
            </interface-class>
            <implementation-class>
                in.oneton.idea.spring.assistant.plugin.component.MavenReIndexingDependencyChangeSubscriberImpl
            </implementation-class>
        </component>
    </project-components>

</idea-plugin>
